
@{
    ViewBag.Title = "AccessDenied";
    <link rel="stylesheet" href="https://www.w3schools.com/w3css/4/w3.css">
    <style>
        h2 {
            color: red;
        }

        h6 {
            color: red;
            text-decoration: underline;
        }
    </style>
}

<div class="Get_inTheMiddle">
    <h2 class="w3-jumbo w3-animate-top w3-center">Access Denied</h2>
    <hr class="w3-border-white w3-animate-left" style="margin:auto;width:50%">
    <h3 class="w3-center w3-animate-right">@ViewBag.User You dont have permission to view this site.</h3>
    <h3 class="w3-center w3-animate-zoom">🚫🚫🚫🚫</h3>
    <p class="w3-center w3-animate-zoom">@ViewBag.ErrorMessage</p>
</div>
